public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int i=0; 
		bucleext: 
			
			while (i <10) {
			i ++ ;
			for (int j = 0; j < i; j++) { 
				System.out.print("*"); 
				
				if (i==5){
					
					break bucleext;
					} 
				
				} //fin for que es el bucle interno
			System.out.println("");
			}//fin del while que es el bucle exterior
	}
