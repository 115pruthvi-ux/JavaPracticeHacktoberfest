public class alumno {
String nombre;
int edad;

alumno(){
	this.nombre ="sin nombre";
	this.edad= 18;
}

alumno(String nom){
	this.nombre= nombre;
	this.edad = edad;
}

public alumno(String nombre, int edad) {
	super();
	this.nombre = nombre;
	this.edad = edad;
}

@Override
public String toString() {
	return "alumno [nombre=" + nombre + ", edad=" + edad + "]";
}


}
