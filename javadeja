	public static void main(String[] args) {
		
	int matematicas = 2, lengua = 2;
	
	if (matematicas>=5) {
		if (lengua >=5) {
			System.out.println("MATES SI // LENGUA SI");
		}else {
			System.out.println("MATES SI // LENGUA NO");
		}
		
	}else if(lengua>=5){
		System.out.println("MATES SI // LENGUA NO");
	}else {
		System.out.println("MATES NO // LENGUA NO");
	}
		
	}
