public class Ejercucui1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int N = 6;
		double A = 2.58;
		char C = 202;
		
		System.out.println("El valor N es igual a " + N);
		System.out.println("El valor A es igual a " + A);
		System.out.println("El valor C es igual a " + C);
		System.out.println("El valor N + A es igual a " + (N + A));
		System.out.println("El valor A - N es igual a " + (A - N));
		System.out.println("El valor num√©rico de C es igual a " + (int)C);
	}

}
